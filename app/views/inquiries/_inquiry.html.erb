<div id="<%= dom_id inquiry %>">
  <p>
    <strong>Name:</strong>
    <%= inquiry.name %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= inquiry.phone_number %>
  </p>

  <p>
    <strong>Email address:</strong>
    <%= inquiry.email_address %>
  </p>

  <p>
    <strong>Message:</strong>
    <%= inquiry.message %>
  </p>

</div>
